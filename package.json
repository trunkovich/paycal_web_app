{
  "name": "paycal-web-app",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --host 0.0.0.0",
    "serve:prod": "ng serve -prod --sourcemap --host 0.0.0.0",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "prefixcss": "postcss -s postcss-scss -u autoprefixer --autoprefixer.browsers \"last 2 versions, iOS > 7, > 1% in US\" -r src/**/*.scss",
    "clone:index": "cp-cli dist/index.html dist/200.html",
    "surge": "surge -p dist -d trunkovich-paycal.surge.sh",
    "build:prod": "ng build -prod --sourcemap",
    "deploy": "npm run build:prod && npm run clone:index && npm run surge"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/cdk": "^2.0.0-beta.11",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/material": "^2.0.0-beta.11",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^4.0.5",
    "@ngrx/store": "^4.0.3",
    "@ngrx/store-devtools": "^4.0.0",
    "angular2-moment": "1.3.3",
    "angular2-text-mask": "2.1.0",
    "angulartics2": "1.6.3",
    "blueimp-canvas-to-blob": "3.6.0",
    "core-js": "2.5.1",
    "cropperjs": "1.0.0-beta.1",
    "intl": "1.2.5",
    "lodash": "4.17.4",
    "mobile-detect": "1.3.5",
    "ngrx-store-freeze": "^0.2.0",
    "ngx-localstorage": "^1.2.0",
    "normalize.css": "5.0.0",
    "proxy-polyfill": "0.1.6",
    "raven-js": "^3.16.1",
    "reselect": "2.5.4",
    "rxjs": "5.4.3",
    "text-mask-addons": "3.1.0",
    "ts-helpers": "1.1.1",
    "zone.js": "0.8.16"
