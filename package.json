{
  "name": "paycal-web-app",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --host 0.0.0.0",
    "serve:prod": "ng serve -prod --sourcemap --host 0.0.0.0",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "prefixcss": "postcss -s postcss-scss -u autoprefixer --autoprefixer.browsers \"last 2 versions, iOS > 7, > 1% in US\" -r src/**/*.scss",
    "clone:index": "cp-cli dist/index.html dist/200.html",
    "surge": "surge -p dist -d trunkovich-paycal.surge.sh",
    "build:prod": "ng build -prod --sourcemap",
    "deploy": "npm run build:prod && npm run clone:index && npm run surge"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.2.3",
    "@angular/cli": "^1.1.2",
    "@angular/common": "4.2.3",
    "@angular/compiler": "4.2.3",
    "@angular/core": "4.2.3",
    "@angular/forms": "4.2.3",
    "@angular/http": "4.2.3",
    "@angular/material": "2.0.0-beta.2",
    "@angular/platform-browser": "4.2.3",
    "@angular/platform-browser-dynamic": "4.2.3",
    "@angular/router": "4.2.3",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.3",
    "@ngrx/store": "2.2.2",
    "@ngrx/store-devtools": "3.2.4",
    "angular2-moment": "1.3.3",
    "angular2-text-mask": "2.1.0",
    "angulartics2": "1.6.3",
    "blueimp-canvas-to-blob": "3.6.0",
    "core-js": "2.4.1",
    "cropperjs": "1.0.0-beta.1",
    "intl": "1.2.5",
    "lodash": "4.17.4",
    "mobile-detect": "1.3.5",
    "ng-http-interceptor": "2.0.0",
    "ngrx-store-freeze": "0.1.6",
    "normalize.css": "5.0.0",
    "proxy-polyfill": "0.1.6",
    "raven-js": "^3.16.1",
    "reselect": "2.5.4",
    "rxjs": "5.4.1",
    "text-mask-addons": "3.1.0",
    "ts-helpers": "1.1.1",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "1.1.2",
    "@angular/compiler-cli": "4.2.3",
    "@types/cropperjs": "0.0.23",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.63",
    "@types/moment": "2.13.0",
    "@types/node": "6.0.42",
    "autoprefixer": "6.6.1",
    "codelyzer": "~2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-remap-istanbul": "0.2.1",
    "postcss": "5.2.10",
    "postcss-cli": "2.6.0",
    "postcss-scss": "0.4.0",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "4.0.2",
