    "@angular/material": "^2.0.0-beta.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/store": "^2.2.1",
    "@types/moment": "^2.13.0",
    "angular2-moment": "^1.1.0",
    "angular2-text-mask": "^2.1.0",
    "core-js": "^2.4.1",
    "lodash": "^4.17.4",
